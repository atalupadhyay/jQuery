					( adsbygoogle = window.adsbygoogle || []).push({});
				</script>
			</div><h2>mousedown</h2>
			Fire when mouse over this element and pressed on the mouse button.
		</div>

		<script type="text/javascript">
			$('#mouseup').mouseup(function() {
				$('#mouseup').slideUp();
			});

			$('#mousedown').mousedown(function() {
				$('#mousedown').fadeOut();
			});

		</script>

